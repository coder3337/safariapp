{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "new-cap": 0,
        "max-len": 0,
        "require-jsdoc": 0,
        "linebreak-style": 0
    }
}